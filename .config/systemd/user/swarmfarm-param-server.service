[Service]
ExecStart=/usr/bin/docker run \
  --rm \
  -v %h/.config/swarmfarm/parameters:/etc/swarmfarm/parameters:ro \
  -v %h/.local/share/swarmfarm/parameters:/var/lib/swarmfarm/parameters:rw \
  --network host \
  -e RUST_LOG=debug \
  --user %U:%G \
  --name swarmfarm-param-server \
  940120858061.dkr.ecr.ap-southeast-2.amazonaws.com/swarmfarm-param-server:1.1.3 \


#  /usr/bin/swarmfarm-parameter-server
ExecStop=/usr/bin/docker stop swarmfarm-param-server
